@{
    Items = @(
        @{
            Name        = 'PSDrives'
            Description = 'Displays a table of registered PSDrives.'
            Image       = 'https://raw.githubusercontent.com/ironmansoftware/scripts/main/images/PowerShell/PowerShell.Widgets/PSDrives.png'
        }
        @{
            Name        = 'Modules'
            Description = 'Displays a table of all modules installed on this computer.'
            Image       = 'https://raw.githubusercontent.com/ironmansoftware/scripts/main/images/PowerShell/PowerShell.Widgets/Modules.png'
        }
        @{
            Name        = 'Repositories'
            Description = 'Displays a table of registered PS resource repositories.'
            Image       = 'https://raw.githubusercontent.com/ironmansoftware/scripts/main/images/PowerShell/PowerShell.Widgets/Repositories.png'
        }
        @{
            Name        = 'PSProviders'
            Description = 'Displays a table of registered PowerShell providers.'
            Image       = 'https://raw.githubusercontent.com/ironmansoftware/scripts/main/images/PowerShell/PowerShell.Widgets/Providers.png'
        }
        @{
            Name        = 'Variables'
            Description = 'Displays a table of variables names and values.'
            Image       = 'https://raw.githubusercontent.com/ironmansoftware/scripts/main/images/PowerShell/PowerShell.Widgets/Variables.png'
        }
        @{
            Name        = 'HostProcesses'
            Description = 'Displays a table of processes hosting PowerShell.'
            Image       = 'https://raw.githubusercontent.com/ironmansoftware/scripts/main/images/PowerShell/PowerShell.Widgets/Host Processes.png'
        }
    )
}